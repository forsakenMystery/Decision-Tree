<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="stage" maxHeight="345.0" maxWidth="600.0" minHeight="345.0" minWidth="600.0" prefHeight="345.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXSOURCE.menu.FXMLController">
   <children>
      <Label alignment="TOP_CENTER" prefHeight="50.0" prefWidth="600.0" text="ID3 Algorithm" textAlignment="CENTER" />
      <TextField disable="true" editable="false" promptText="Symptom file.." />
      <HBox>
         <children>
            <Button fx:id="chooseSymptomFile" mnemonicParsing="false" onMouseClicked="#chooseSymptomFileClicked" prefWidth="700.0" text="Choose Symptom file" />
            <Button fx:id="defaultSymptomFile" alignment="TOP_CENTER" defaultButton="true" disable="true" mnemonicParsing="false" onMouseClicked="#defaultSymptomFileClicked" prefWidth="700.0" text="Default Symptom file" />
         </children>
      </HBox>
      <TextField disable="true" editable="false" promptText="Disease files..." />
      <HBox>
         <children>
            <Button fx:id="chooseDiseaseFile" mnemonicParsing="false" onMouseClicked="#chooseDiseaseFileClicked" prefHeight="25.0" prefWidth="700.0" text="Choose Disease file" />
            <Button fx:id="defaultDiseaseFile" defaultButton="true" disable="true" mnemonicParsing="false" onMouseClicked="#defaultDiseaseFileClicked" prefHeight="25.0" prefWidth="700.0" text="Default Disease file" />
         </children>
      </HBox>
      <Label prefHeight="56.0" prefWidth="600.0" />
      <Button fx:id="about" minWidth="700.0" mnemonicParsing="false" onMouseClicked="#aboutClicked" prefHeight="25.0" prefWidth="700.0" text="About" />
      <Label prefHeight="45.0" prefWidth="600.0" />
      <Button fx:id="start" defaultButton="true" minWidth="700.0" mnemonicParsing="false" onMouseClicked="#startClicked" prefHeight="25.0" prefWidth="700.0" text="Start the Algorithm" />
      <Label prefHeight="45.0" prefWidth="600.0" />
      <Button fx:id="exit" minWidth="700.0" mnemonicParsing="false" onMouseClicked="#exitClicked" prefHeight="25.0" prefWidth="700.0" text="Exit" />
   </children>
</VBox>
